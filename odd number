package baekjoon;

import java.util.Scanner;

public class open {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        if (n % 2 == 0 || n <= 0) {
            System.out.println("홀수가 아니거나 양의 정수가 아닙니다");
        } else {
            int sum = 0;
            for (int i = 1; i <= n; i+= 2) {
                sum += i;
            }
            System.out.println("홀수들의 합 : " + sum);
        }
    }
}
